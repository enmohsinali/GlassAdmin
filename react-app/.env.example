# GlassAdmin Environment Variables
# Copy this file to .env and fill in your values

# Application Settings
VITE_APP_NAME=GlassAdmin
VITE_APP_VERSION=1.0.0

# API Configuration
# VITE_API_BASE_URL=https://api.example.com
# VITE_API_TIMEOUT=30000

# Authentication (if using external auth service)
# VITE_AUTH_DOMAIN=your-auth-domain.auth0.com
# VITE_AUTH_CLIENT_ID=your-client-id
# VITE_AUTH_AUDIENCE=your-api-audience

# Analytics (if using analytics service)
# VITE_ANALYTICS_ID=your-analytics-id
# VITE_ANALYTICS_ENABLED=true

# Feature Flags
# VITE_FEATURE_DARK_MODE=true
# VITE_FEATURE_I18N=true
# VITE_FEATURE_ANIMATIONS=true

# External Services
# VITE_STRIPE_PUBLIC_KEY=pk_test_...
# VITE_GOOGLE_MAPS_API_KEY=...

# Development Settings
# VITE_DEV_TOOLS=true
# VITE_LOG_LEVEL=debug

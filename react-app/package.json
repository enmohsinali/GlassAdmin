  },
  "bugs": {
    "url": "https://github.com/enmohsinali/GlassAdmin/issues",
    "email": "support@glassadmin.com"
  },
  "keywords": [
    "react",
    "admin",
    "dashboard",
    "template",
    "glassmorphism",
    "glass-ui",
    "ios-26",
    "liquid-glass",
    "tailwindcss",
    "vite",
    "admin-template",

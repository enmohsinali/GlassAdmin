    "moment": "^2.30.1",
    "react": "^19.1.1",
    "react-apexcharts": "^1.8.0",
    "react-big-calendar": "^1.19.4",
    "react-dom": "^19.1.1",
    "react-dropzone": "^14.3.8",
    "react-hook-form": "^7.66.0",
    "react-hot-toast": "^2.6.0",
    "react-i18next": "^16.2.4",
    "react-router-dom": "^7.9.5",
    "react-select": "^5.10.2",
    "recharts": "^3.3.0",
    "tailwindcss-rtl": "^0.9.0",
    "yup": "^1.7.1"
  },

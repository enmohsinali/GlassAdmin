  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "a11y": "pa11y http://localhost:5173/components --standard WCAG2AA",
    "a11y:ci": "pa11y-ci",
    "a11y:components": "pa11y http://localhost:5173/components --standard WCAG2AA --reporter cli",
    "a11y:json": "pa11y http://localhost:5173/components --standard WCAG2AA --reporter json > pa11y-report.json",
    "a11y:html": "pa11y http://localhost:5173/components --standard WCAG2AA --reporter html > pa11y-report.html",
    "a11y:all": "npm run dev & sleep 5 && pa11y-ci && killall node"
  },
